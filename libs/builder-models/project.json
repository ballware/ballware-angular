{
  "name": "builder-models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/builder-models/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/builder-models",
        "main": "libs/builder-models/src/index.ts",
        "tsConfig": "libs/builder-models/tsconfig.lib.json",
        "assets": ["libs/builder-models/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs builder-models {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/builder-models/jest.config.ts"
      }
    }
  },
  "tags": []
}
