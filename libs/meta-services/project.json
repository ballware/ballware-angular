{
  "name": "meta-services",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/meta-services/src",
  "prefix": "ballware",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "dist/libs/meta-services"
      ],
      "options": {
        "project": "libs/meta-services/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/meta-services/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/meta-services/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/meta-services"
      ],
      "options": {
        "jestConfig": "libs/meta-services/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/meta-services/**/*.ts",
          "libs/meta-services/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
